#include <iostream>
using namespace std;

int main() {
    int n = 5; 
    int n1 = 0; 
    int n2 = 0;
    int arr[n]; 
    int even[n]; 
    int odd[n]; 
  
    
    cout << "Enter " << n << " integers: ";
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    for (int i = 0; i < n; i++) {
        if (arr[i] % 2 == 0) {
            even[n1] = arr[i];
            n1++;
        } else {
            odd[n2] = arr[i];
            n2++;
        }
    }
    for (int i = 0; i < n1; i++) {
        cout << even[i] << " ";
    }
  
    for (int i = 0; i < n2; i++) {
        cout << odd[i] << " ";
    }
     return 0;
}
